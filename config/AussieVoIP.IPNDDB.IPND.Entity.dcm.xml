<!-- config/Product.dcm.xml -->
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
	http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

	<entity name="AussieVoIP\IPNDDB\IPND\Entity" table="entities">
		<id name="id" type="integer">
			<generator strategy="AUTO" />
		</id>

		<field name="Type" type="string" length="12" />
		<field name="Title" type="string" length="12" nullable="true" />
		<field name="Rawname" type="string" length="160" />
		<field name="Surname" type="string" length="40" nullable="true" />
		<field name="Firstname" type="string" length="40" nullable="true" />
		<field name="Longname" type="string" length="80" nullable="true" />
		<field name="Contactnum" type="string" length="30" nullable="true" />
	</entity>
</doctrine-mapping>

